{
  "name": "school-clinic-workspace",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently -n server,client -c green,cyan \"npm --prefix server run dev\" \"npm --prefix client run dev -- --host\"",
    "dev:server": "npm --prefix server run dev",
    "dev:client": "npm --prefix client run dev -- --host",
    "build": "npm --prefix client run build"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
